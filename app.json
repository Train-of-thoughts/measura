{
  "expo": {
    "name": "measura",
    "slug": "measura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assets": [
      "./assets/"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFBF6"
      },
      "package": "com.anonymous.measura"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Geologica_Black.otf",
            "./assets/fonts/Geologica_Bold.otf",
            "./assets/fonts/Geologica_ExtraBold.otf",
            "./assets/fonts/Geologica_ExtraLight.otf",
            "./assets/fonts/Geologica_Light.otf",
            "./assets/fonts/Geologica_Medium.otf",
            "./assets/fonts/Geologica_Regular.otf",
            "./assets/fonts/Geologica_SemiBold.otf",
            "./assets/fonts/Geologica_Thin.otf"
          ]
        }
      ],
      [
        "expo-asset",
        {
          "assets": "./assets"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
